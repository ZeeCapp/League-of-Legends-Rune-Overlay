{
  "dependencies": {
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "http-proxy-middleware": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "tcp-port-used": "^1.0.2"
  },
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "build": "npm-run-all -p build:* -s package",
    "start": "node build/server/main.js",
    "dev:tsc": "tsc -w",
    "dev:webpack": "webpack serve --mode=development",
    "dev:server": "nodemon --watch build/server build/server/main.js --env dev",
    "build:server": "tsc",
    "build:client-app": "webpack --mode=production",
    "package": "pkg build/server/main.js --out-path dist -c package.json"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/minimist": "^1.2.2",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/tcp-port-used": "^1.0.1",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "minimist": "^1.2.5",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.5.2",
    "ts-loader": "^9.2.8",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "pkg": {
    "assets": "build/client-app/**/*",
    "outputPath": "dist"
  }
}
